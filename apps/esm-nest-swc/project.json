{
  "name": "esm-nest-swc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/esm-nest-swc/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": ["^build"],
      "options": {
        "cwd": "apps/esm-nest-swc",
        "command": "nest build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/esm-nest-swc",
        "commands": [
          "nx run esm-nest-swc:build --watch",
          "nx run esm-nest-swc:typecheck --batch --watch",
          "node --watch dist/main.js"
        ]
      }
    }
  }
}
