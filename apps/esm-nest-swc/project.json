{
  "name": "esm-nest-swc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/esm-nest-swc/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/esm-nest-swc",
        "commands": ["nest build", "nx run esm-nest-swc:typecheck --batch"]
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/esm-nest-swc",
        "commands": ["nx run esm-nest-swc:build --watch", "node --watch dist/main.js"]
      }
    }
  }
}
