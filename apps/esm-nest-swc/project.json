{
  "name": "esm-nest-swc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/esm-nest-swc/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": ["^build", "typecheck"],
      "options": {
        "cwd": "apps/esm-nest-swc",
        "command": "nest build"
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "build"
      }
    }
  }
}
